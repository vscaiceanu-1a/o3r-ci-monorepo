{
  "name": "my-sdk",
  "version": "0.0.0-placeholder",
  "description": "my-sdk - SDK",
  "source": "src/index.ts",
  "main": "cjs/index.js",
  "module": "esm2020/index.js",
  "node": "cjs/index.js",
  "es2020": "esm2020/index.js",
  "esm2020": "esm2020/index.js",
  "es2015": "esm2015/index.js",
  "esm2015": "esm2015/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "main": "./cjs/index.js",
      "module": "./esm2020/index.js",
      "es2020": "./esm2020/index.js",
      "esm2020": "./esm2020/index.js",
      "node": "./cjs/index.js",
      "default": "./esm2020/index.js",
      "typings": "./index.d.ts",
      "import": "./esm2020/index.js",
      "require": "./esm2020/index.js"
    }
  },
  "scripts": {
    "schematics": "schematics",
    "clean": "rimraf test/ test-dev/ dist/ dist-dev/ dist-test/ build/",
    "lint:ci": "eslint \"**/*[jt]s\" --quiet --format junit --output-file ./dist-lint/result.xml",
    "lint": "eslint \"**/*[jt]s\" --cache",
    "start": "tsc-watch -b tsconfigs/esm2020 --noClear --onFirstSuccess \"npm run files:pack --watch\"",
    "build": "npm run build:cjs && npm run build:esm2015 && npm run build:esm2020 && npm run files:pack",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q --strip-leading-paths",
    "build:esm2015": "swc src -d dist/esm2015 -C module.type=es6 -q --strip-leading-paths",
    "build:esm2020": "tsc -b tsconfigs/esm2020",
    "resolve": "node -e 'process.stdout.write(require.resolve(process.argv[1]));'",
    "generate": "schematics @ama-sdk/schematics:typescript-core",
    "spec:regen": "npm run generate -- --generator-key -my-sdk && amasdk-clear-index",
    "spec:upgrade": "npm run spec:regen",
    "files:pack": "amasdk-files-pack",
    "test": "jest --passWithNoTests",
    "publish:package": "npm publish ./dist",
    "generate:mock": "schematics @ama-sdk/schematics:mock",
    "doc:generate": "typedoc",
    "tools:changelog": "commit-and-tag-version"
  },
  "lint-staged": {
    "**/*.(js|ts)": [
      "npm exec eslint --quiet --fix --global fast"
    ]
  },
  "dependencies": {
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.6.2"
  },
  "peerDependenciesMeta": {
    "isomorphic-fetch": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ama-sdk/core": "~10.4.1",
    "@ama-sdk/schematics": "~10.4.1",
    "@angular-devkit/core": "~17.3.0",
    "@angular-devkit/schematics": "~17.3.0",
    "@angular-devkit/schematics-cli": "~17.3.0",
    "@angular-eslint/eslint-plugin": "~17.5.0",
    "@angular-eslint/eslint-plugin-template": "~17.5.0",
    "@angular-eslint/utils": "~17.5.0",
    "@angular/cli": "~17.3.0",
    "@angular/compiler": "~17.3.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@o3r/eslint-config-otter": "~10.4.1",
    "@o3r/eslint-plugin": "~10.4.1",
    "@o3r/schematics": "~10.4.1",
    "@openapitools/openapi-generator-cli": "~2.13.0",
    "@schematics/angular": "~17.3.0",
    "@stylistic/eslint-plugin-ts": "^1.5.4",
    "@swc/cli": "~0.3.0",
    "@swc/core": "~1.5.24",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "commit-and-tag-version": "^9.5.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-node": "",
    "eslint-plugin-jest": "~27.9.0",
    "eslint-plugin-jsdoc": "~48.2.1",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "globby": "^11.1.0",
    "isomorphic-fetch": "~3.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "lint-staged": "^13.0.0",
    "minimist": "^1.2.6",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ts-jest": "~29.1.2",
    "tsc-watch": "^6.0.0",
    "typedoc": "~0.25.0",
    "typescript": "~5.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "peerDependencies": {
    "@ama-sdk/core": "~10.4.1",
    "isomorphic-fetch": "~3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": "",
    "yarn": ""
  }
}
